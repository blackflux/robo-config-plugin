{
  "target": "README.md",
  "strategy": "merge-below-title",
  "snippets": [
    {
      "name": "circleci-private.md.mustache",
      "variables": {
        "repoKey": "${repoKey}",
        "circleCiReadToken": "${circleCiReadToken}"
      }
    }
  ],
  "requires": [
    "circleci"
  ],
  "purpose": [
    "Display [circleci](https://circleci.com/) badge for private repo in README."
  ]
}

{
  "target": ".gally.json",
  "strategy": "overwrite",
  "snippets": [
    {
      "name": "two-branch",
      "variables": {
        "repoKey": "${repoKey}",
        "mergeDevUsers": [
          "${owner}",
          "${mergeBot}"
        ],
        "dismissalUsers": [
          "${owner}"
        ],
        "releaseUsers": [
          "${owner}"
        ],
        "requiredStatusChecks": [
          "ci/circleci: gally-create-pr",
          "ci/circleci: test-node-v10",
          "ci/circleci: test-node-v8.10"
        ],
        "requiredReviewCount": 1
      }
    }
  ],
  "requires": [
    "circleci",
    "gally",
    "github"
  ],
  "purpose": [
    "Restrictive two-branch [gally](https://github.com/loopmediagroup/gally#readme) config. Custom for npm open source packages.",
    "TODO ..."
  ]
}

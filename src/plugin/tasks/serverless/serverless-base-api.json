{
  "target": "serverless/.base/api.yml",
  "format": "txt",
  "strategy": "overwrite",
  "snippets": [
    {
      "name": "serverless/.base/api.yml.mustache",
      "variables": {
        "nccEnabled": "${nccEnabled}",
        "nccExclude": "${nccExclude}"
      }
    }
  ],
  "requires": [
    "serverless",
    "aws"
  ],
  "purpose": [
    "Manage serverless api stack base configuration file."
  ]
}

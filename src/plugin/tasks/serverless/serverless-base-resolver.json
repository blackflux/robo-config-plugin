{
  "target": "serverless/.base/resolver.yml",
  "format": "yml",
  "strategy": "overwrite",
  "snippets": [
    {
      "name": "serverless/.base/resolver.yml.mustache",
      "variables": {
        "nccEnabled": "${nccEnabled}",
        "projectName": "${projectName}",
        "slsEndpointType": "${slsEndpointType}",
        "namespace": "${namespace}"
      }
    }
  ],
  "requires": [
    "serverless",
    "aws"
  ],
  "purpose": [
    "Create serverless stack resolver configuration file."
  ]
}

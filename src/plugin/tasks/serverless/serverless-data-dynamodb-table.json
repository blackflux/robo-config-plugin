{
  "target": "serverless/data/dynamodb/${table}.yml",
  "format": "yml",
  "strategy": "merge-deep",
  "snippets": [
    {
      "name": "serverless/dynamodb/data/${table}.yml",
      "variables": {
        "table": "${table|TITLE}",
        "tableStreamEnabled": "${tableStreamEnabled}"
      }
    }
  ],
  "requires": [
    "serverless",
    "aws"
  ],
  "purpose": [
    "Initialize data stack resource definitions for an dynamodb table."
  ]
}

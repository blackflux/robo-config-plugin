{
  "target": "serverless/data/sqs/${queue}.yml",
  "format": "yml",
  "strategy": "merge-deep",
  "snippets": [
    {
      "name": "serverless/sqs/data/${queue}.yml",
      "variables": {
        "queue": "${queue|TITLE}",
        "queueVisibilityTimeout": "${queueTimeout}",
        "fifoQueue": "${fifoQueue}"
      }
    }
  ],
  "requires": [
    "serverless",
    "aws"
  ],
  "purpose": [
    "Initialize data stack resource definitions for an sqs queue."
  ]
}

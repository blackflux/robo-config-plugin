{
  "defaultBranch": "master",
  "contribBranch": "dev",
  "repository": {"url": "https://github.com/repoKey.git"},
  "branches": {
    "dev": {"protection": "$dev", "create": true, "upstream": "stage"},
    "stage": {"protection": "$stage", "create": true, "upstream": "master"},
    "master": {"protection": "$master", "create": true},
    "fix/*": {"upstream": "dev"},
    "feat/*": {"upstream": "dev"},
    "dependabot/*": {"upstream": "dev"}
  },
  "protection": {
    "$dev": {
      "@": "$full",
      "required_status_checks": {"strict": true, "contexts": []},
      "restrictions": {
        "users": ["owner", "mergeBot"],
        "apps": ["dependabot-preview"]
      }
    },
    "$stage": {
      "@": "$full",
      "required_status_checks": {
        "contexts": [
          "ci/circleci: deploy-local",
          "ci/circleci: deploy-dev",
          "ci/circleci: test-node-v18.x",
          "ci/circleci: test-node-v18.x-lib",
          "ci/circleci: test-node-v20"
        ]
      }
    },
    "$master": {
      "@": "$full",
      "required_status_checks": {
        "contexts": [
          "ci/circleci: deploy-stage",
          "ci/circleci: test-node-v18.x",
          "ci/circleci: test-node-v18.x-lib",
          "ci/circleci: test-node-v20"
        ]
      }
    },
    "$full": {
      "required_status_checks": {
        "strict": false,
        "contexts": [
          "ci/circleci: test-node-v18.x",
          "ci/circleci: test-node-v18.x-lib",
          "ci/circleci: test-node-v20"
        ]
      },
      "enforce_admins": true,
      "required_pull_request_reviews": {
        "dismissal_restrictions": {"users": ["owner"], "teams": []},
        "dismiss_stale_reviews": true,
        "require_code_owner_reviews": true,
        "required_approving_review_count": 1
      },
      "restrictions": {"users": ["owner"], "teams": [], "apps": []}
    }
  }
}
